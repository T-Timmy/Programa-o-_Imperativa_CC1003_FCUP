# include <stdio.h>

int main (void) {

  int a, b, c;

  printf("Quais os comprimentos :" ); scanf("%d , %d , %d", &a, &b, &c );

  if (a == b && a == c) {
    printf("O triângulo é equilátero\n");
  } else if (a == b || a == c || b == c) {
    printf("O triângulo é isósceles \n");
  } else {
    printf("O triângulo é escaleno \n");
  }

return 0;
}
