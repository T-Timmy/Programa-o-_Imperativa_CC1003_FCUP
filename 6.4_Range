# include <stdio.h>

void range (int vec[], unsigned size, int inicio, int incr) {

  for (int i=0; i<size; i++){

    vec[i] = inicio + (i*incr);

    printf("%u\n", vec[i] );
  }
}

int main (void) {

  int n, ini, inc;

  printf("Qual o tamanho: "); scanf("%d", &n);
  printf("Qual o inicio: "); scanf("%d", &ini);
  printf("Qual o incremento: "); scanf("%d", &inc);


  int a[n];

  range (a, n, ini, inc);

  return 0;
}
