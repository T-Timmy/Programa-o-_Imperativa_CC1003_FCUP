# include <stdio.h>
# define FALSE 0
# define TRUE 1


int primo (int a){

  if(a <=1){
    return FALSE;
  } else {
    for (int i=2; i<a; i++){
      if (a%i == 0){
        return FALSE;
      }
    }
  }
  return TRUE;
}

int main (void){

  int n;

  printf("Qual o limite: "); scanf("%d", &n);

  for (int j=0; j<=n; j++){
    if(primo(j)){
      printf("O %d Ã© primo\n", j);
    }
  }

  return 0;
}
